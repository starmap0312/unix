# list a folder
  hadoop fs -ls /path/to/hadoop/folder

# download a hadoop folder
  hadoop fs -get /path/to/folder

# upload a local folder to hadoop
  hadoop fs -put /path/to/local/folder/ /path/to/hadoop/folder

# cat a hadoop file
  hadoop fs -cat /path/to/filename

#
  hadoop fs -count -q /path/to/hadoop/folder
